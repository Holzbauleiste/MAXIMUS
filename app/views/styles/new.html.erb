<head>
<title>New Style</title>
</head>
<body>
<center>
<% if !user_signed_in? %>
  <%= form_for :styles, url: styles_path, html: { multipart: true } do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
      <%= file_field_tag "styles[]", type: :file, multiple: true %>
    </p>
 
    <%= f.file_field :pic %>
    <p>
      <%= f.label :text %><br>
      <%= f.text_area :text %>
    </p>
 
    <p>
      <%= f.submit %>
    </p>
  <% end %>
  <% else %>
<% end %>
</center>
</body>